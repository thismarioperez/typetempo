{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "db:seed": {
            "inputs": [".env*"],
            "cache": false
        },
        "db:generate": {
            "inputs": [".env*"],
            "cache": false
        },
        "db:push": {
            "inputs": [".env*"],
            "cache": false
        },
        "db:migrate": {
            "inputs": [".env*"],
            "cache": false,
            "persistent": true
        },
        "db:reset": {
            "inputs": [".env*"],
            "cache": false,
            "persistent": true
        },
        "db:studio": {
            "inputs": [".env*"],
            "cache": false
        },
        "build": {
            "dependsOn": ["db:generate"],
            "inputs": ["$TURBO_DEFAULT$", ".env*"],
            "outputs": ["dist/**"]
        },
        "lint": {},
        "dev": {
            "dependsOn": ["db:generate"],
            "cache": false,
            "persistent": true
        },
        "start": {
            "dependsOn": ["^build"],
            "inputs": [".env*"]
        }
    }
}
